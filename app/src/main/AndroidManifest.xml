<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.jumpstreet.spacetrader">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.ConfigurationActivity"></activity>
        <activity android:name=".view.SolarSystemActivity"></activity>
        <activity android:name=".view.UniverseActivity"></activity>
        <activity android:name=".view.PlanetActivity"></activity>
        <activity android:name=".view.MarketPlaceActivity"></activity>
        <activity android:name=".view.MarketPlaceTradePopupActivity"></activity>
    </application>

</manifest>